generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String
  members  Person[]
}

model Person {
  id       String  @id @default(uuid())
  name     String
  relation String
  addedBy  User    @relation(fields: [addedById], references: [id])
  addedById String
}